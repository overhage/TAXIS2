{
"compilerOptions": {
"target": "ESNext",
"useDefineForClassFields": true,
"lib": ["DOM", "ESNext"],
"module": "ESNext",
"moduleResolution": "Bundler",
"types": ["@netlify/functions", "node"],
"strict": true,
"jsx": "react-jsx",
"noEmit": true,
"skipLibCheck": true,
"esModuleInterop": true,
"allowSyntheticDefaultImports": true
},
"include": ["src", "netlify/functions", "vite-env.d.ts"]
}